{
  "name": "LineLengthBot",
  "version": "1.0.0",
  "description": "Limits Discord messages to a set line and character limit",
  "main": "bot.js",
  "scripts": {
    "once": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "pm2 start bot.js --name LineLength",
    "stop": "pm2 stop bot.js",
    "restart": "pm2 restart bot,js",
    "fix": "npx eslint . --fix",
    "lint": "npx eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/detjensrobert/LineLengthBot.git"
  },
  "author": "Robert Detjens / wholewheatbagels",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/detjensrobert/LineLengthBot/issues"
  },
  "homepage": "https://github.com/detjensrobert/LineLengthBot#readme",
  "dependencies": {
    "chalk": "^3.0.0",
    "discord.js": "^11.5.1",
    "pm2": "^4.2.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0"
  }
}
